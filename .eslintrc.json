{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["tsconfig.json"],
        "parser": {
            "ts": "@typescript-eslint/parser",
            "<template>": "espree"
        }
    },
    "plugins": [
        "vue"
    ],
    "rules": {
    }
}
